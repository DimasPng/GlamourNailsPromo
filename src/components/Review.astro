---
import {Image} from 'astro:assets'
import disposable from '../assets/images/mainPage/disposable.svg'
import review1 from '../assets/images/mainPage/review1.png'
import review2 from '../assets/images/mainPage/review2.png'
import review3 from '../assets/images/mainPage/review3.png'
import review4 from '../assets/images/mainPage/review4.png'
import review5 from '../assets/images/mainPage/review5.png'
import review6 from '../assets/images/mainPage/review6.png'
import play from '../assets/images/mainPage/play.png'
--- 

<section class="bg-neutral-50">
    <div class="container  flex flex-row mx-auto justify-between">
            <span class="text-neutral-600 text-2xl">74% naszych klientek<br/>przychodzi z polecenia.</span>
            <button class="text-white text-lg h-8 w-28 bg-pink-500 rounded-2xl">Instagram</button>
            </div>

            <div class="container flex mx-auto flex-row justify-around">
                <Image class="p-4" src={review1} alt="Video review1"/>
                <Image class="p-4" src={review2} alt="Video review2"/>
                <Image class="p-4" src={review3} alt="Video review3"/>
            </div>
            <div class="container flex flex-row mx-auto justify-around">
                <Image class="p-4" src={review4} alt="Video review4"/>
                <Image class="p-4" src={review5} alt="Video review5"/>
                <Image class="p-4" src={review6} alt="Video review6"/>



            </div>







        </section>









   
